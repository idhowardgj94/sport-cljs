{
  "scripts": {
    "start": "yarn dev",
    "rollup:release": "rollup -c build/rollup/rollup.config.js",
    "gulp:release": "gulp -f build/gulpfile.js build",
    "shadow:watch": "shadow-cljs watch dev",
    "shadow:release": "shadow-cljs release app",
    "test": "shadow-cljs compile test",
    "postcss:build": "cross-env TAILWIND_MODE=build postcss src/css/tailwind.scss -o ./public/css/main.css --verbose",
    "postcss:watch": "cross-env TAILWIND_MODE=watch postcss src/css/tailwind.scss -o ./public/css/main.css --verbose -w",
    "postcss:release": "cross-env NODE_ENV=production postcss src/css/tailwind.scss -o ./public/css/main.css --verbose",
    "dev": "run-p -l *:watch",
    "release": "run-s *:release",
    "deploy": "firebase deploy"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "autoprefixer": "^10.4.7",
    "cssnano": "^5.1.9",
    "gulp-cli": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-postcss": "^4.0.2",
    "shadow-cljs": "^2.19.0",
    "tailwindcss": "^3.0.24"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@headlessui/react": "^1.6.2",
    "@tailwindcss/forms": "^0.5.2",
    "create-react-class": "15.7.0",
    "cross-env": "^7.0.3",
    "date-and-time": "^2.3.1",
    "date-fns": "^2.28.0",
    "firebase": "9.8.1",
    "gulp": "^4.0.2",
    "postcss-import": "^14.1.0",
    "postcss-scss": "^4.0.4",
    "postcss-url": "^10.1.3",
    "react": "17.0.1",
    "react-date-picker": "^8.4.0",
    "react-dom": "17.0.1",
    "regenerator-runtime": "^0.13.9",
    "rollup": "^2.76.0",
    "rollup-plugin-terser": "^7.0.2"
  }
}
